-- Trigger: tr_eggsbatchexplorations_create

-- DROP TRIGGER tr_eggsbatchexplorations_create ON public."EggsBatchExplorations";

CREATE TRIGGER tr_eggsbatchexplorations_create
    BEFORE INSERT OR UPDATE 
    ON public."EggsBatchExplorations"
    FOR EACH ROW
    EXECUTE FUNCTION public.tr_fc_eggsbatchexplorations_create();