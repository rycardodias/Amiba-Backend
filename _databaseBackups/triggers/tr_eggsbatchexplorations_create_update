-- Trigger: tr_eggsbatchexplorations_create_update

-- DROP TRIGGER tr_eggsbatchexplorations_create_update ON public."EggsBatchExplorations";

CREATE TRIGGER tr_eggsbatchexplorations_create_update
    BEFORE INSERT OR UPDATE 
    ON public."EggsBatchExplorations"
    FOR EACH ROW
    EXECUTE FUNCTION public.tr_fc_eggsbatchexplorations_create_update();