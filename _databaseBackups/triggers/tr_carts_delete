-- Trigger: tr_carts_delete

-- DROP TRIGGER tr_carts_delete ON public."Carts";

CREATE TRIGGER tr_carts_delete
    BEFORE INSERT OR UPDATE 
    ON public."Carts"
    FOR EACH ROW
    EXECUTE FUNCTION public.tr_fc_carts_delete();

COMMENT ON TRIGGER tr_carts_delete ON public."Carts"
    IS 'Add the quantity from the row back to quantityAvailable';